package groupg.quiztime;

import java.util.Random;

public class Question {
	public void Question (){
		_stub = "Make a default question";
	}
	private String _stub;
	private String _dummy1;
	private String _dummy2;
	private String _dummy3;
	private String _correct;
	
	public String getStub(){
		return _stub;
	}
	
	public String[] getAnswerArray() {
		String[] answers = new String[4];
		
		Random r = new Random();

		answers[r.nextInt(0-4) + 0] = _dummy1;//sent answers at position of random int between 0-3 (+ 0 is inclusive)
		
		
		
		return null;
	}

}
